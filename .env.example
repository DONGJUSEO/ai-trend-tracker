# 데이터베이스 설정
DATABASE_URL=postgresql://postgres:postgres@db:5432/ai_trends

# Redis 설정
REDIS_URL=redis://redis:6379/0

# OpenAI API 키 (GPT-4o-mini 사용)
# https://platform.openai.com/api-keys 에서 발급받으세요
OPENAI_API_KEY=

# Hugging Face API 키
# https://huggingface.co/settings/tokens 에서 발급받으세요
HUGGINGFACE_API_KEY=

# Gemini API 키 (무료!)
# https://ai.google.dev/gemini-api/docs/api-key 에서 발급받으세요
GEMINI_API_KEY=your_api_key_here

# YouTube Data API 키
YOUTUBE_API_KEY=your_api_key_here

# GitHub Personal Access Token
GITHUB_TOKEN=your_github_token_here

# 스케줄링 설정 (12시간마다)
SCHEDULER_INTERVAL_HOURS=12

# 애플리케이션 설정
APP_NAME=AI Trend Tracker
APP_VERSION=0.1.0
DEBUG=True

# Gemini API 키 추가됨

# 앱 접속 비밀번호
APP_PASSWORD=test1234
